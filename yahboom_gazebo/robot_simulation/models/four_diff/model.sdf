    <!-- Rear Right Wheel (zh_Link) -->
    <link name="zh_Link">
      <pose>-0.0495 0.0675 -0.02125 0 0 0</pose>
      <inertial>
        <pose>0 0 0 1.57 0 0</pose>
        <inertia>
          <ixx>2.66315588150881E-06</ixx>
          <ixy>1.26558270834104E-10</ixy>
          <ixz>5.60449660674614E-10</ixz>
          <iyy>4.68480910884624E-06</iyy>
          <iyz>-8.82832301592923E-12</iyz>
          <izz>2.66433206935428E-06</izz>
        </inertia>
        <mass>0.0141019726113614</mass>
      </inertial>

      <collision name="zh_collision">
        <pose>0 0 0 1.57 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.0325</radius>
            <length>0.018</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
              <fdir1>0 1 0</fdir1>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <soft_cfm>0</soft_cfm>
