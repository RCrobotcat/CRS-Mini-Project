    <!-- Rear Right Wheel (zh_Link) -->
    <link name="zh_Link">
      <pose>-0.0495 0.0675 -0.02125 0 0 0</pose>
      <inertial>
        <pose>2.71005454374126E-05 0.00150864011455075 -1.08539409463503E-05 0 0 0</pose>
        <mass>0.0141019726113614</mass>
        <inertia>
          <ixx>2.66315588150881E-06</ixx>
          <ixy>1.26558270834104E-10</ixy>
          <ixz>5.60449660674614E-10</ixz>
          <iyy>4.68480910884624E-06</iyy>
          <iyz>-8.82832301592923E-12</iyz>
          <izz>2.66433206935428E-06</izz>
        </inertia>
      </inertial>
      <visual name="zh_visual">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/zh_Link.STL</uri>
