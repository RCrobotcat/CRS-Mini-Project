    <!-- ========== IMU Link ========== -->
    <link name="imu_Link">
      <pose>-0.002999 -0.0030001 0.031701 0 0 0</pose>
      <inertial>
        <pose>-0.0316761324986627 0.00132159782250217 -0.000479349648167576 0 0 0</pose>
        <mass>0.0132101454078263</mass>
        <inertia>
          <ixx>2.09255982058915E-06</ixx>
          <ixy>-1.5860144299643E-08</ixy>
          <ixz>-3.27485068549135E-10</ixz>
          <iyy>4.73790359695282E-06</iyy>
          <iyz>-2.29375131893769E-10</iyz>
          <izz>6.79434472621533E-06</izz>
        </inertia>
      </inertial>
      <visual name="imu_visual">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/imu_Link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.149 0.149 0.149 1</ambient>
          <diffuse>0.149 0.149 0.149 1</diffuse>
        </material>
      </visual>
      <collision name="imu_collision">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/imu_Link.STL</uri>
          </mesh>
        </geometry>
      </collision>

      <!-- IMU Sensor -->
      <sensor name="imu_sensor" type="imu">
        <always_on>true</always_on>
        <update_rate>100</update_rate>
        <visualize>true</visualize>
        <topic>imu</topic>
        <imu>
          <angular_velocity>
            <x>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>2e-4</stddev>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>2e-4</stddev>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>2e-4</stddev>
              </noise>
            </z>
          </angular_velocity>
          <linear_acceleration>
            <x>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>1.7e-2</stddev>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>1.7e-2</stddev>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0.0</mean>
                <stddev>1.7e-2</stddev>
              </noise>
            </z>
          </linear_acceleration>
        </imu>
      </sensor>
    </link>

    <!-- ========== Radar/Lidar Link ========== -->
    <link name="radar_Link">
      <pose>0 0 0.078933557468359 0 0 0</pose>
      <inertial>
        <pose>2.50028066700073E-05 9.8943368066196E-05 -0.0175251744352531 0 0 0</pose>
        <mass>0.0384696758414604</mass>
        <inertia>
          <ixx>8.81880784590578E-06</ixx>
          <ixy>-1.66165059019566E-07</ixy>
          <ixz>-1.11024960466139E-07</ixz>
          <iyy>1.44967275960782E-05</iyy>
          <iyz>-3.88415222298422E-08</iyz>
          <izz>2.24335777017028E-05</izz>
        </inertia>
      </inertial>
      <visual name="radar_visual">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/radar_Link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.180 0.180 0.180 1</ambient>
          <diffuse>0.180 0.180 0.180 1</diffuse>
        </material>
      </visual>
      <collision name="radar_collision">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/radar_Link.STL</uri>
          </mesh>
        </geometry>
      </collision>

      <!-- Lidar Sensor -->
      <sensor name="lidar" type="ray">
        <pose>0 0 0 0 0 0</pose>
        <always_on>true</always_on>
        <visualize>false</visualize>
        <update_rate>10</update_rate>
        <topic>scan</topic>
        <ray>
          <scan>
            <horizontal>
              <samples>360</samples>
              <resolution>1</resolution>
              <min_angle>-3.14159</min_angle>
              <max_angle>3.14159</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.12</min>
            <max>12.0</max>
            <resolution>0.015</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
      </sensor>
    </link>

    <!-- ========== Robotic Arm Links (Optional) ========== -->
    <!-- Joint 1 Link (jq1_Link) -->
    <link name="jq1_Link">
      <pose>0.064 0 0.014 0 0 0</pose>
      <inertial>
        <pose>0.0110585433790032 -0.00605208841063553 0.015781881952638 0 0 0</pose>
        <mass>0.0115074341427154</mass>
        <inertia>
          <ixx>1.61581689683415E-06</ixx>
          <ixy>1.00926034806027E-09</ixy>
          <ixz>-2.6905085628935E-07</ixz>
          <iyy>9.62368700578607E-07</iyy>
          <iyz>7.36350494885753E-09</iyz>
          <izz>1.35577194148353E-06</izz>
        </inertia>
      </inertial>
      <visual name="jq1_visual">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/jq1_Link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.776 0.757 0.737 1</ambient>
          <diffuse>0.776 0.757 0.737 1</diffuse>
        </material>
      </visual>
      <collision name="jq1_collision">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/jq1_Link.STL</uri>
          </mesh>
        </geometry>
      </collision>
    </link>

    <!-- Joint 2 Link (jq2_Link) -->
    <link name="jq2_Link">
      <pose>0.080864 8.4892E-05 0.037959 0 0 0</pose>
      <inertial>
        <pose>0.0135058163511177 0.000857671482029795 0.00288177408889561 0 0 0</pose>
        <mass>0.0133313425901211</mass>
        <inertia>
          <ixx>1.96303534926057E-06</ixx>
          <ixy>3.68224074620386E-09</ixy>
          <ixz>2.60821852145315E-07</ixz>
          <iyy>1.5250310537691E-06</iyy>
          <iyz>5.421881574463E-09</iyz>
          <izz>1.9926579435283E-06</izz>
        </inertia>
      </inertial>
      <visual name="jq2_visual">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/jq2_Link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.298 0.298 0.298 1</ambient>
          <diffuse>0.298 0.298 0.298 1</diffuse>
        </material>
      </visual>
      <collision name="jq2_collision">
        <geometry>
          <mesh>
            <uri>model://yahboomcar_description/meshes/jq2_Link.STL</uri>
          </mesh>
        </geometry>
      </collision>
    </link>

    <!-- ========== Joints ========== -->
    <!-- Wheel Joints -->
    <joint name="zq_Joint" type="revolute">
      <parent>base_link</parent>
      <child>zq_Link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="yq_Joint" type="revolute">
      <parent>base_link</parent>
      <child>yq_Link</child>
      <axis>
        <xyz>0 -1 0</xyz>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="yh_Joint" type="revolute">
      <parent>base_link</parent>
      <child>yh_Link</child>
      <axis>
        <xyz>0 -1 0</xyz>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="zh_Joint" type="revolute">
      <parent>base_link</parent>
      <child>zh_Link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <!-- IMU Joint -->
    <joint name="imu_Joint" type="fixed">
      <parent>base_link</parent>
      <child>imu_Link</child>
    </joint>

    <!-- Radar Joint -->
    <joint name="radar_Joint" type="fixed">
      <parent>base_link</parent>
      <child>radar_Link</child>
    </joint>

    <!-- Arm Joints -->
    <joint name="jq1_Joint" type="revolute">
      <parent>base_link</parent>
      <child>jq1_Link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
          <effort>10</effort>
          <velocity>1.0</velocity>
        </limit>
        <dynamics>
          <damping>0.5</damping>
          <friction>0.5</friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="jq2_Joint" type="revolute">
      <parent>jq1_Link</parent>
      <child>jq2_Link</child>
      <axis>
        <xyz>0.0037845 -0.99999 0</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
          <effort>10</effort>
          <velocity>1.0</velocity>
        </limit>
        <dynamics>
          <damping>0.5</damping>
          <friction>0.5</friction>
        </dynamics>
      </axis>
    </joint>

    <!-- ========== Gazebo Plugins ========== -->
    <!-- Four-Wheel Differential Drive Plugin -->
    <plugin name="diff_drive" filename="libgazebo_ros_diff_drive.so">
      <!-- Wheels -->
      <left_joint>yq_Joint</left_joint>
      <left_joint>yh_Joint</left_joint>
      <right_joint>zq_Joint</right_joint>
      <right_joint>zh_Joint</right_joint>

      <!-- Kinematics -->
      <wheel_separation>0.135</wheel_separation>
      <wheel_diameter>0.065</wheel_diameter>

      <!-- Limits -->
      <max_wheel_torque>5.0</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>

      <!-- Output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <publish_wheel_tf>false</publish_wheel_tf>

      <!-- Topics -->
      <command_topic>cmd_vel</command_topic>
      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>

      <!-- Update Rate -->
      <update_rate>50</update_rate>
    </plugin>

    <!-- Joint State Publisher -->
    <plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
      <update_rate>50</update_rate>
      <joint_name>zq_Joint</joint_name>
      <joint_name>yq_Joint</joint_name>
      <joint_name>yh_Joint</joint_name>
      <joint_name>zh_Joint</joint_name>
      <joint_name>jq1_Joint</joint_name>
      <joint_name>jq2_Joint</joint_name>
    </plugin>

  </model>
</sdf>
